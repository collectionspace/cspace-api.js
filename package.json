{
  "name": "cspace-api",
  "version": "0.0.1",
  "description": "CollectionSpace services API wrapper for browsers and Node.js",
  "author": "Ray Lee <rhlee@berkeley.edu>",
  "license": "ECL-2.0",
  "keywords": [
    "collectionspace",
    "cspace",
    "service",
    "resource",
    "rest",
    "api"
  ],
  "main": "src/index.js",
  "scripts": {
    "doc": "documentation readme src/*.js -s \"API\"",
    "lint": "eslint *.js src test examples",
    "build": "npm run build-dev && npm run build-prod",
    "build-dev": "webpack",
    "build-prod": "cross-env NODE_ENV=production webpack",
    "test": "npm run test-node-specs && npm run test-browser-specs",
    "test-specs": "npm run test-node-specs && npm run test-browser-specs",
    "test-browser": "npm run test-browser-specs && npm run test-browser-integration",
    "test-node": "npm run test-node-specs && npm run test-node-integration",
    "test-node-specs": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive test/specs",
    "test-node-integration": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive --slow 250 test/integration",
    "test-browser": "npm run test-browser-specs && npm run test-browser-integration",
    "test-browser-specs": "cross-env NODE_ENV=test karma start --singleRun=true --dir=specs",
    "test-browser-integration": "cross-env NODE_ENV=test karma start --singleRun=true --dir=integration",
    "start": "cross-env NODE_ENV=test karma start"
  },
  "devDependencies": {
    "babel-core": "^6.13.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.1",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "cross-env": "^2.0.0",
    "deep-freeze-strict": "^1.1.1",
    "documentation": "^4.0.0-beta9",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "karma": "^1.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sauce-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "extend": "^3.0.0"
  }
}
